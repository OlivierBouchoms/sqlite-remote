{
    "name": "sqlite-remote",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 4000",
        "build": "vite build",
        "generate:api": "openapi-rq -i http://localhost:7000/swagger/v1/swagger.json -c axios -o src/generated/api",
        "lint:staged": "lint-staged",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "style": "prettier . --check --ignore-unknown",
        "style:fix": "prettier . --write --ignore-unknown",
        "ts": "tsc -p . --noEmit"
    },
    "lint-staged": {
        "**/*": [
            "prettier --write --ignore-unknown",
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@radix-ui/themes": "^3.2.1",
        "@tanstack/react-query": "^5.89.0",
        "@tanstack/react-query-devtools": "^5.90.2",
        "@vitejs/plugin-react": "^5.0.4",
        "axios": "^1.12.2",
        "i18next": "^23.12.1",
        "interactjs": "^1.10.27",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.52.1",
        "react-i18next": "^14.1.3",
        "react-icons": "^5.5.0",
        "react-router-dom": "^6.25.0",
        "vite": "^7.2.4"
    },
    "devDependencies": {
        "@7nohe/openapi-react-query-codegen": "^1.6.2",
        "@eslint/js": "^9.17.0",
        "@openapitools/openapi-generator-cli": "^2.25.2",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "globals": "^15.14.0",
        "lint-staged": "^15.3.0",
        "prettier": "3.4.2",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.19.0"
    }
}
